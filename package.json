{
  "name": "amapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn start",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --passWithNoTests",
    "eject": "react-scripts eject",
    "install:functions": "npm --prefix functions i",
    "deploy": "NODE_ENV=development yarn build && bash scripts/confirm.sh && firebase deploy --project=default",
    "deploy:func": "firebase deploy --project=default --only functions",
    "deploy:rules": "firebase deploy --project=default --only firestore:rules"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "firebase": "^7.21.0",
    "node-sass": "^4.14.1",
    "polished": "^4.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-firebase-hooks": "^2.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "recompose": "^0.30.0",
    "rxjs": "^6.6.3",
    "styled-components": "^5.2.1",
    "typescript": "~3.7.2"
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.5",
    "@types/recompose": "^0.30.7",
    "@types/styled-components": "^5.1.7"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
